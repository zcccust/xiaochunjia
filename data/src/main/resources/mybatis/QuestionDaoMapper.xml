<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaochunjia.data.dao.QuestionDao">
	<select id="getAll" resultType="com.xiaochunjia.data.model.Question">
		select * from question;
	</select>
	<select id="get" resultType="com.xiaochunjia.data.model.Question">
		select * from question where id=#{0};
	</select>
</mapper>